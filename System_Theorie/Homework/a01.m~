% Global variables
i = 50; % index of an array with numbers
z = 100; % lenght of zero array

% PART A ===============================
% The vector A with first 50 natural digits 
a_ = zeros([1 i]); % array prealocation 
for a = 1:i
    a_(1, a) = a;
end
% show created array A
a_

%================================
% Reversed vector B
b_ = fliplr(a_)

% Array of all zeros, lenght is 100 elements
c_ = zeros([1 z])

